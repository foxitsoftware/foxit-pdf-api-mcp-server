{
  "name": "@foxitsoftware/foxit-pdf-api-mcp-server",
  "version": "0.2.3",
  "mcpName": "io.github.foxitsoftware/foxit-pdf-api-mcp-server",
  "private": false,
  "main": "dist/index.js",
  "bin": {
    "foxit-pdf-api-mcp-server": "dist/stdio-server.js"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/server.js",
    "dev": "fastmcp dev src/stdio-server.ts",
    "lint": "prettier --check . && eslint . && tsc --noEmit",
    "test": "vitest run",
    "format": "prettier --write . && eslint --fix .",
    "inspect": "fastmcp inspect dist/index.js"

  },
  "keywords": [
    "fastmcp",
    "mcp",
    "Foxit PDF API"
  ],
  "author": "Foxit Software Inc. <support@foxitsoftware.com>",
  "homepage": "https://developer-api.foxit.com",
  "type": "module",
  "license": "MIT",
  "description": "MCP server for Foxit PDF API",
  "dependencies": {
    "fastmcp": "^3.20.0",
    "zod": "^3.24.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^24.10.1",
    "@wong2/mcp-cli": "1.13.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-perfectionist": "^4.12.3",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "tsup": "^8.5.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^4.0.0"
  }
}
